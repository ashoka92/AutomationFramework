		<parameter name="udid" value="emulator-5556"></parameter>
		<parameter name="systemPort" value="10002"></parameter>
		<parameter name="chromeDriverPort" value="11002"></parameter>
		<parameter name="testdata" value="loginUsers.json"></parameter>
		<classes>
			<class name="com.qa.automation.test.LoginAppTest" />
			<class name="com.qa.automation.test.ProductTest" />
		</classes>
	</test>
</suite> <!-- Suite -->
